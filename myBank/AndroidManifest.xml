<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybank"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mybank.MainActivity"
            android:label="@string/app_name" >
            <intent-filter android:label="@string/app_name" >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mybank.BookingActivity"
            android:label="@string/title_activity_booking" >
        </activity>
        <activity
            android:name="com.example.mybank.settings.SettingsProfileActivity"
            android:label="@string/title_activity_settings_profile" >
        </activity>
        <activity
            android:name="com.example.mybank.settings.SettingsNotificationsActivity"
            android:label="@string/title_activity_settings_notifications" >
        </activity>
        <activity
            android:name="com.example.mybank.settings.SettingsBankingActivity"
            android:label="@string/title_activity_settings_banking" >
        </activity>
        <activity
            android:name="com.example.mybank.HistoryActivity"
            android:label="@string/title_activity_history" >
        </activity>
        <activity
            android:name="com.example.mybank.OutlayActivity"
            android:label="@string/title_activity_outlay" >
        </activity>
        <activity
            android:name="com.example.mybank.ChartActivity"
            android:label="@string/title_activity_chart" >
        </activity>
        <activity
            android:name="com.example.mybank.NotifyService"
            android:label="@string/title_activity_notify_service" >
        </activity>
        <activity
            android:name="com.example.mybank.CustomDialogActivity"
            android:label="@string/title_activity_custom_dialog" >
        </activity>
        <activity
            android:name="com.example.mybank.BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
    </application>

</manifest>
